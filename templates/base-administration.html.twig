<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}Administration{% endblock %}</title>
        {% block stylesheets %}
            <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500" rel="stylesheet">
            <link rel="stylesheet" href="{{ asset('css/administration.css') }}">
            <link rel="stylesheet" href="{{ asset('libs/bootstrap/bootstrap.css') }}">
            <link rel="stylesheet" href="{{ asset('libs/fontawesome/fontawesome.min.css') }}">
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset('libs/jquery/jquery-3.1.1.min.js') }}"></script>
            <script src="{{ asset('libs/bootstrap/bootstrap.js') }}"></script>
        {% endblock %}
    </body>
</html>
